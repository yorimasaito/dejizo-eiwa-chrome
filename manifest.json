{
    "name": "eiwa-chrome",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "デ地蔵APIからシュッと英和辞書検索をする拡張機能",
    "icons": {
        "16": "./images/icon16.png",
        "48": "./images/icon48.png",
        "128": "./images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "./images/icon19.png",
            "38": "./images/icon38.png"
        },
        "default_title": "Dejizo Waei Chrome",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "http://*/*",
        "https://*/*"
    ]
}

