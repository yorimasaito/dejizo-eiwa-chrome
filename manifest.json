{
    "name": "Dejizo Eiwa Chrome",
    "version": "1.0.3",
    "manifest_version": 2,
    "author": "tanishiking",
    "description": "英和辞書検索を簡単にするための拡張機能 この拡張機能には デ辞蔵REST版APIを利用しています.",
    "icons": {
        "16": "./images/icon16.png",
        "48": "./images/icon48.png",
        "128": "./images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "./images/icon19.png",
            "38": "./images/icon38.png"
        },
        "default_title": "Dejizo Eiwa Chrome",
        "default_popup": "./popup/popup.html"
    },
    "permissions": [
        "http://public.dejizo.jp/NetDicV09.asmx/*"
    ]

}

